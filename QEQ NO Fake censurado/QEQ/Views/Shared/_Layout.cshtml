<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Bienvenido al Quien Es Quien</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/MiCSS.css" type="text/css" rel="stylesheet">
    <script src="~/Scripts/alertify.js"></script>
    <script src="~/Scripts/alertify.min.js"></script>
    <link href="~/Content/alertify.bootstrap.css" rel="stylesheet" />
    <link href="~/Content/alertify.core.css" rel="stylesheet" />
    <link href="~/Content/alertify.default.css" rel="stylesheet" /> </head>
<body>
    <div class="navbar navbar-color navbar-fixed-top">
        <div class="container miNav">

            <div class="navbar-collapse collapse">
                <ul class="navbar-nav">
                    <li> <a class="titulo" href="/Home/Index">Inicio</a> </li>

                    <li>@Html.ActionLink("Juego", "TypeGame", "Game")</li>
                    <li>@Html.ActionLink("Desarrolladores", "Desarrolladores", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
